{
    "name": "jarviswebapp",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "server": "nodemon",
        "client": "npm start --prefix jarvis-app",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "build": "npm run build --prefix jarvis-app"
    },
    "dependencies": {
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "morgan": "~1.9.1"
    },
    "devDependencies": {
        "concurrently": "^7.2.2",
        "nodemon": "^2.0.16"
    }
}
